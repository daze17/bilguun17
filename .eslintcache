User = \"dark\" in localStorage;\n    const savedMode = JSON.parse(localStorage.getItem('dark'));\n    const userPrefersDark = getPrefColorSheme();\n    if (isReturningUser){\n        return savedMode;\n    } else if (userPrefersDark) {\n        return true;\n    } else {\n        return false;\n    }\n}\n  function getPrefColorSheme(){\n      if(!window.matchMedia) return;\n      return window.matchMedia(\"(prefers-color-scheme: dark)\").matches;\n  }\n\n  useEffect(()=>{\n    Aos.init({ duration: 500 })\n  }, [])\n\n  return (\n    <Router>\n      <div onScroll={handleScroll} className=\"App\">\n        <Switch>\n          <Route exact path=\"/\">\n            <NavBar toggleDrawer={() => toggleOpen()} scroll={navScroll} darkMode={darkMode} setDarkMode={setDarkMode} /> \n            <SideDrawer\n                isShown={isOpen}\n                closeDrawer={() => toggleOpen()}\n                setDarkMode={setDarkMode}\n                darkMode={darkMode}\n                />\n            <HomePage/>\n            <About darkMode={darkMode}/>\n            <Works/>\n            <Contact/>\n            <Footer/>\n          </Route>\n          <Route component={NotFoundPage} />\n        </Switch>\n      </div>\n    </Router>\n  );\n}\n\nexport default App;\n","D:\\react\\blgn\\src\\components\\HomePage\\HomePage.js",[],"D:\\react\\blgn\\src\\components\\About\\About.js",[],"D:\\react\\blgn\\src\\components\\NavBar\\NavBar.js",[],"D:\\react\\blgn\\src\\components\\Works\\Works.js",[],"D:\\react\\blgn\\src\\components\\Skills\\Skills.js",[],"D:\\react\\blgn\\src\\components\\Contact\\Contact.js",[],"D:\\react\\blgn\\src\\components\\Footer\\Footer.js",[],"D:\\react\\blgn\\src\\components\\NavBar\\DrawerToggleButton.js",[],"D:\\react\\blgn\\src\\components\\Works\\ProjectCard.js",[],"D:\\react\\blgn\\src\\components\\SideDrawer\\SideDrawer.js",[],"D:\\react\\blgn\\src\\components\\SideDrawer\\CloseButton.js",[],"D:\\react\\blgn\\src\\components\\SideDrawer\\MenuItems.js",[